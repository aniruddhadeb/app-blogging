{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve",
    "paths": {
      "@shared/ui/button": [
        "./src/app/shared/ui/button/src/index.ts"
      ],
      "@shared/ui/utils": [
        "./src/app/shared/ui/utils/src/index.ts"
      ],
      "@shared/ui/card": [
        "./src/app/shared/ui/card/src/index.ts"
      ],
      "@shared/ui/input": [
        "./src/app/shared/ui/input/src/index.ts"
      ],
      "@shared/ui/label": [
        "./src/app/shared/ui/label/src/index.ts"
      ],
      "@shared/ui/separator": [
        "./src/app/shared/ui/separator/src/index.ts"
      ],
      "@shared/ui/avatar": [
        "./src/app/shared/ui/avatar/src/index.ts"
      ],
      "@shared/ui/dialog": [
        "./src/app/shared/ui/dialog/src/index.ts"
      ],
      "@shared/ui/icon": [
        "./src/app/shared/ui/icon/src/index.ts"
      ],
      "@shared/ui/alert": [
        "./src/app/shared/ui/alert/src/index.ts"
      ],
      "@shared/ui/sonner": [
        "./src/app/shared/ui/sonner/src/index.ts"
      ],
      "@shared/ui/skeleton": [
        "./src/app/shared/ui/skeleton/src/index.ts"
      ],
      "@shared/ui/textarea": [
        "./src/app/shared/ui/textarea/src/index.ts"
      ],
      "@shared/ui/form-field": [
        "./src/app/shared/ui/form-field/src/index.ts"
      ],
      "@shared/ui/spinner": [
        "./src/app/shared/ui/spinner/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
